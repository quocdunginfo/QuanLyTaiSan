<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Microsoft.SyncFramework.DatabaseProvider</id>
    <version>3.1.0.0</version>
    <title>Microsoft Sync Framework Database Providers Redistributable</title>
    <authors>Darrell Tunnell</authors>
    <owners>Darrell Tunnell</owners>
    <projectUrl>http://www.microsoft.com/en-gb/download/details.aspx?id=19502</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This package will ensure that the 'DatabaseProviders' msi for MS Sync Framework is installed, and it will then add references to the necessary assemblies:
      Microsoft.Synchronization.Data.dll,
      Microsoft.Synchronization.Data.Server.dll,
      Microsoft.Synchronization.Data.SqlServer.dll,
      Microsoft.Synchronization.Data.SqlServerCe.dll.
      The msi distributables included in this package are obtained from here: http://www.microsoft.com/en-gb/download/details.aspx?id=19502</description>
    <summary>Installs the necessary development dependencies for the Microsoft Sync Framework Database Providers.</summary>
    <releaseNotes />
    <language>en-gb</language>
    <tags>Sync Framework SyncFramework Microsoft MicrosoftSync MicrosoftSyncFramework Redistributable SyncFx MS DatabaseProvider Database Provider SyncProvider SqlCe SqlServer</tags>
    <dependencies>
      <dependency id="Microsoft.SyncFramework" version="2.1.0.2" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="Microsoft.Synchronization.Data" targetFramework="" />
      <frameworkAssembly assemblyName="Microsoft.Synchronization.Data.Server" targetFramework="" />
      <frameworkAssembly assemblyName="Microsoft.Synchronization.Data.SqlServer" targetFramework="" />
      <frameworkAssembly assemblyName="Microsoft.Synchronization.Data.SqlServerCe" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
</package>